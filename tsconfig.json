  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext",
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["ES6", "DOM"]
  },
  "include": ["src/**/*.tsx"],
  "exclude": ["node_modules"]
}
